pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
               
                git branch: 'main', url: 'https://github.com/joshua-mnz/College'
            }
        }

        stage('Compile') {
            steps {
                echo 'Compiling Java program...'
                sh 'javac Demo.java'
            }
        }

        stage('Run Java') { // Renamed for clarity
            steps {
                echo 'Running Java program...'
                sh 'java Demo'
            }
        }
}
    post {
        success {
            echo 'Build and execution successful!'
        }
        failure {
            echo 'Build failed!'
        }
    }
}
