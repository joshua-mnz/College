pipeline{
	agent any
	triggers {
CRON ('* * * * *')
}
	stages {
		stage('Checkout'){
				steps{
					git branch : 'main', url: 'https://github.com/joshua-mnz/College'
					}
				}
		stage('Build'){
				steps{
					echo "Building …."
					sh 'python3 hello.py'
					}
				}
		stage('Test'){
				steps{
					echo "Testing…"
					}
				}
		}
	post{
success{
	echo "Succesfully Running..."
	}
failure{
	echo "Build Failed"
	}
}
}
